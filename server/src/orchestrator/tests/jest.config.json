{
  "displayName": "Orchestrator Tests",
  "testMatch": [
    "<rootDir>/src/orchestrator/tests/**/*.spec.ts"
  ],
  "collectCoverageFrom": [
    "src/orchestrator/**/*.ts",
    "!src/orchestrator/**/*.spec.ts",
    "!src/orchestrator/**/*.e2e-spec.ts",
    "!src/orchestrator/tests/**/*",
    "!src/orchestrator/**/*.config.ts"
  ],
  "coverageDirectory": "<rootDir>/coverage/orchestrator",
  "coverageReporters": [
    "text",
    "lcov",
    "html"
  ],
  "testEnvironment": "node",
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../..",
  "testRegex": ".*\\.spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}